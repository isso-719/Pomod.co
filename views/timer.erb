<head>
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
	<meta name="viewport" content="width=device-width">
	<title>Timer - Pomod.co</title>
	<meta name="description" content="Pomod.co(ポモドコ)はオシャレで集中できるポモドーロタイマーです。このアプリは25分作業して5分休むことで集中力を継続することのできるポモドーロ勉強法を基にして作られています。">
	<meta itemprop="name" content="Pomod.co">
	<meta itemprop="image" content="https://pomod.co/assets/images/ogp.png">
	<link rel="canonical" href="https://pomod.co/timer" />
	<link rel="alternate" type="application/rss+xml" href="https://pomod.co/assets/sitemap.xml" />
	<!-- OGP -->
	<meta property="fb:app_id" content="1203442269812622">
	<meta property="og:url" content="https://pomod.co/timer">
	<meta property="og:type" content="website">
	<meta property="og:title" content="Pomod.co">
	<meta property="og:description" content="Pomod.co(ポモドコ)はオシャレで集中できるポモドーロタイマーです。このアプリは25分作業して5分休むことで集中力を継続することのできるポモドーロ勉強法を基にして作られています。">
	<meta property="og:image" content="https://pomod.co/assets/images/ogp.png">
	<!-- Twitter -->
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:title" content="Pomod.co">
	<meta name="twitter:description" content="Pomod.co(ポモドコ)はオシャレで集中できるポモドーロタイマーです。このアプリは25分作業して5分休むことで集中力を継続することのできるポモドーロ勉強法を基にして作られています。">
	<meta name="twitter:image" content="https://pomod.co/assets/images/ogp.png">
	<meta name="twitter:site" content="@i5so_" />
	<!-- CSS -->
	<link rel="stylesheet" href="assets/css/timer.css" />
	<!-- ファビコン -->
	<link rel="icon" href="assets/images/favicon.ico">
	<!-- スマホ用アイコン -->
	<link rel="apple-touch-icon" sizes="180x180" href="assets/images/app_icon_180.png">
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<meta name="apple-mobile-web-app-status-bar-style" content="default">
	<!-- Windows用アイコン -->
	<meta name="application-name" content="Pomod.co" />
	<meta name="msapplication-square70x70logo" content="assets/images/app_icon_70.png" />
	<meta name="msapplication-square150x150logo" content="assets/images/app_icon_150.png" />
	<meta name="msapplication-square310x310logo" content="assets/images/app_icon_310.png" />
	<meta name="msapplication-TileColor" content="#efeeee" />
	<!-- テーマカラー -->
	<meta name="theme-color" content="#efeeee">
</head>

<body>
	<main>
		<div class="container">
			<div class="card">
				<div class="class">
					<div class="percent">
						<div class="number">
							<svg>
								<defs>
									<linearGradient id="linear" x1="0%" y1="0%" x2="100%" y2="100%">
										<stop offset="0%" opacity="100%" stop-color="#00ff48" />
										<stop offset="100%" opacity="100%" stop-color="#00e63c" />
									</linearGradient>
								</defs>
								<circle class="inner" cx="140" cy="140" r="140" stroke="rgba(0, 0, 0, 0.64)"></circle>
								<circle class="outer" cx="140" cy="140" r="140" stroke="url(#linear)"></circle>
							</svg>
							<div id="indicator">
								<h5>残り時間</h5>
								<h2 id="timer"></h2>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</main>
	<div id="controller">
		<button id="start" aria-label="start"><i class="fas fa-play"></i></button>
		<button id="stop" aria-label="stop" disabled><i class="fas fa-pause"></i></button>
		<a id="href" href="/interval"><button id="back" aria-label="back"><i class="fas fa-stop"></i></button></a>
		<br>
		<button id="plusMin" aria-label="plusMin">+1min</button>
		<button id="minusMin" aria-label="minusMin">-1min</button>
	</div>

	<audio id="alarm" preload="auto">
		<source src="assets/audio/alarm.mp3" type="audio/mp3">
	</audio>
	<video style="display: none;" muted autoplay id="sleep-prevent-video-ios" playsinline webkit-playsinline="1">
		<source src="data:video/mp4;base64,">
	</video>
	<video style="display: none;" muted autoplay id="sleep-prevent-video-android" playsinline webkit-playsinline>
		<source src="data:video/mp4;base64,">
	</video>


	<script>
		(function (d) {
			var c = d.createElement('link');
			c.type = 'text/css';
			c.rel = 'stylesheet';
			c.href = 'https://use.fontawesome.com/releases/v5.0.6/css/all.css';
			var s = d.getElementsByTagName('script')[0];
			s.parentNode.insertBefore(c, s);
		})(document);
	</script>
	<script src="https://code.jquery.com/jquery-3.4.1.min.js" integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo=" crossorigin="anonymous"></script>
	<script>
		$(window).on("resize orientationchange", function (e) {
			var wsw = window.screen.width;
			if (wsw <= 360) {
				//デバイス横幅360以下
				$("meta[name='viewport']").attr("content", "width=400");
			} else {
				//それ以外
				$("meta[name='viewport']").attr("content", "width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no,shrink-to-fit=no");
			}
		}).trigger("resize");
	</script>
	<script src="assets/js/timer.js"></script>
	<script src="assets/js/antisleep.js" defer></script>
	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-84261059-4"></script>
	<script>
		window.dataLayer = window.dataLayer || [];
		function gtag() { dataLayer.push(arguments); }
		gtag('js', new Date());

		gtag('config', 'UA-84261059-4');
	</script>
</body>
</html>