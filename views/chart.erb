  <script>
    var line = document.getElementById('line').getContext('2d');
    var chart = new Chart(line, {
      // 作成したいチャートタイプ
      type: 'line',

      // データセットの情報
      data: {
        labels: ["6日前", "5日前", "4日前", "3日前", "2日前", "昨日", "今日"],
        datasets: [{
          label: "勉強時間",
          borderColor: 'rgb(255, 99, 132)',
          data: [0, 10, 5, 2, 20, 30, 45],
        }]
      },

      // 設定オプションはここに
      options: {
        responsive: true,
        maintainAspectRatio: false
      }
    });
  </script>
